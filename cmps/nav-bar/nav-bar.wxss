/* cmps/nav-bar/nav-bar.wxss */
.container {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 99;
  }
.nav-bar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    font-size: 32rpx;
    font-weight: bold;
}

.icons {
    position: absolute;
    left: 0;
    top: 0;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    height: 100%;
}

.icons image {
    width: 50rpx;
    height: 50rpx;
}

.home {
    margin-left: 30rpx;
}